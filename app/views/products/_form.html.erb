<%= form_with model: @product do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :brand %>
    <%= form.text_field :brand %>
  </div>

  <div>
    <%= form.label :price %>
    <%= form.text_field :price %>
  </div>
  
  <%= form.submit %>
<% end %>